{
  "name": "epidemia",
  "version": "0.0.0",
  "private": true,
  "description": "Pandemic Epidemic Forecaster",
  "scripts": {
    "spec": "jest",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pspeter3/epidemia.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pspeter3/epidemia/issues"
  },
  "homepage": "https://github.com/pspeter3/epidemia#readme",
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3"
  },
  "jest": {
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true,
        "skipBabel": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "_spec\\.(ts|tsx)$",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
